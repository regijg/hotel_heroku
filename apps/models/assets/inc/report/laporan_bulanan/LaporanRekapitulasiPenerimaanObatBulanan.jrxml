	<queryString>
		<![CDATA[SELECT tpd.penerimaan_detail_kode as kodeObat, tpd.penerimaan_detail_nama_obat as namaObat,
tpd.penerimaan_detail_satuan as satuan,
SUM(tpd.penerimaan_detail_qty) as qty,
SUM(tso.satuan_obat_harga_jual*tpd.penerimaan_detail_qty) as jumlahHarga
FROM trx_penerimaan_detail tpd
LEFT OUTER JOIN trx_penerimaan_head tph on tpd.penerimaan_detail_head_id = tph.penerimaan_head_id
LEFT OUTER JOIN tbl_obat tbo on tpd.penerimaan_detail_obat_id = tbo.obat_id
LEFT OUTER JOIN tbl_satuan_obat tso on tpd.penerimaan_detail_satuan_obat_id = tso.satuan_obat_id
LEFT OUTER JOIN tbl_jenis_obat tjo on tbo.obat_jenis_obat_id = tjo.jenis_obat_id
WHERE tpd.penerimaan_detail_status = 0 AND to_char(tph.penerimaan_head_tanggal, 'YYYY-MM') = $P{bulanTahun}
AND
CASE
    WHEN $P{paramJenisObat} = '' THEN tjo.jenis_obat_nama = '' OR tjo.jenis_obat_nama is null
    ELSE tjo.jenis_obat_nama = $P{paramJenisObat}
 END
GROUP BY tpd.penerimaan_detail_kode, tpd.penerimaan_detail_nama_obat,
tpd.penerimaan_detail_satuan]]>
	</queryString>
